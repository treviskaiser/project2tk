    </ul>
  </div>
</div>

<div class="row">
  <div class="col-sm">
    <div class="card" id="monday">
      <div class="card-body">
        <h5 class="card-title text-center" id="day0"></h5>
        <p class="card-text">
          {{#each calendar}}
          {{#if dates}}
          <ul>{{this.tim}}
            {{this.appointment}}</ul>
          {{/if}}
          {{/each}}

        </p>
        <div id="weather-0"></div>
        <div id="holiday-0"></div>
      </div>
    </div>
  </div>
  <div class="col-sm">
    <div class="card" id="tuesday">
