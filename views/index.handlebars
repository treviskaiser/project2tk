<div data-aos="zoom-in-up" data-aos-offset="200" data-aos-delay="50" data-aos-duration="1000"
  data-aos-easing="ease-in-out" data-aos-mirror="true" data-aos-once="false" data-aos-anchor-placement="top-center">
  <div class="jumbotron jumbotron-fluid">
    <div class="jumbo-container">
      <h1 class="display-4">Calendar</h1>
      <p class="lead">Plan your day the smart way.</p>
    </div>
  </div>

  <div class="month-container">
    <div class="month">
      <ul>
        <li class="prev">&#10094;</li>
        <li class="next">&#10095;</li>
        <li>
          <div id="month"></div>
          <span style="font-size:18px" id="year"></span>
        </li>
      </ul>
    </div>

    <ul class="weekdays">
      <li>Su</li>
      <li>Mo</li>
      <li>Tu</li>
      <li>We</li>
      <li>Th</li>
      <li>Fr</li>
      <li>Sa</li>

    </ul>

    <ul class="days">
      <li></li>
